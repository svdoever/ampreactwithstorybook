    "url": "https://github.com/svdoever/ampreactwithstorybook.git"
  },
  "homepage": "https://github.com/svdoever/ampreactwithstorybook.git#readme",
  "devDependencies": {
    "@storybook/react": "^3.2.13",
    "babel-eslint": "^8.0.1",
